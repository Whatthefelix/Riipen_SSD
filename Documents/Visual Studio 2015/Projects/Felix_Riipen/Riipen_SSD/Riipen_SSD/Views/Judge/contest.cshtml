@model IEnumerable<Riipen_SSD.ViewModels.TeamCriteriaScoreVM>

@{
    ViewBag.Title = "contest";
}

<h2>contest</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CriteriaID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YourCurrentScore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FinalScore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JudgeNotSubmitted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CriteriaID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YourCurrentScore)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FinalScore)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JudgeNotSubmitted)
        </td>
        <td>
            @Html.ActionLink("View", "Team", new { teamID = item.TeamID}) 
        </td>
    </tr>
}

</table>
